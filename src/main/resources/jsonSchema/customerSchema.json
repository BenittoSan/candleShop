{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "CustomerSchema",
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "email": {
      "type": "string",
      "format": "email"
    },
    "birthday": {
      "type": "string",
      "format": "date"
    }
  },
  "required": ["name", "lastName", "email", "birthday"]
}
