{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ProductInventorySchema",
  "type": "object",
  "properties": {
    "productName": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "quantity": {
      "type": "integer"
    },
    "price": {
      "type": "number"
    },
    "weight": {
      "type": "number"
    },
    "burnDuration": {
      "type": "integer"
    },
    "categoryID": {
      "type": "integer"
    }
  },
  "required": ["productName", "description", "quantity", "price","weight","burnDuration", "categoryID"]
}
